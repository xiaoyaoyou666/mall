<script type="text/javascript">
	$(document).ready(function(){
		if($(".end_time_s").val()=="1970-01-01 08:00:00"){
			$(".end_time_s").val("")
		}
		if($(".expire_time_s").val()=="1970-01-01 08:00:00"){
			$(".expire_time_s").val("")
		}
		//供应商展示
		if($(".uls_li").children("li").length!=0&&$(".uls_li").children("li").attr("name")!=""){
			$(".noe_left").show();
			$(".cont").show();		
			$(".sky_us").show();
			$(".sp_b").show();
