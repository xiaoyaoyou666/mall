			$(".uls_li li img").show();
		};
		if($("#purch tbody tr td").children("span").text()!=""){
            $("#purch tbody tr td").children("input").hide();
            $("#purch tbody tr td").children("span").show();
            $(".sub_mit").hide();
            $(".revision_s").show()
		}
		$(".pvw").height($(window).height()-200);
		$(".mele").height($(window).height()-100);
		if($(".xunjiadan_url").val()==""){
			$(".dele_a").hide()
		}
		if($(".businessfile_url").val()==""){
			$(".dele_b").hide()
		}
		if($(".other_url").val()==""){
			$(".dele_c").hide()
		}
		$(".dele_a").click(function(){
			$(".xunjia_sc").text("");
			$(".xunjiadan_url").val("");
			$(this).hide()
		});
		$(".dele_b").click(function(){
			$(".xunjia_sw").text("");
			$(".businessfile_url").val("");
			$(this).hide()
		});
		$(".dele_c").click(function(){
			$(".xunjia_qt").text("");
			$(".other_url").val("");
			$(this).hide()
		})
		$(function(){
			$(window).unbind('beforeunload');
			window.onbeforeunload = null;
		});
		if($(".uls_li").children("li").length>8){
            	$(".uls_li").height(210)
            }
	})
</script>
